IPCHECKING_API_ENDPOINT=""              # IPCheck.ing 的 API 端點 URL

# Default IP Geo Source
VITE_DEFAULT_IP_GEO_SOURCE="3"          # 預設 IP 地理位置信息來源 (0=IPCheck.ing, 1=IPinfo.io, 3=IPAPI.is)

# Security related
SECURITY_BLACKLIST_LOG_FILE_PATH=""     # 記錄觸發限制的 IP 列表的文件路徑
SECURITY_RATE_LIMIT=""                  # 控制每 60 分鐘一個 IP 可以對後端服務器請求的次數（0 為不限制）
SECURITY_DELAY_AFTER=""                 # 控制每 20 分鐘一個 IP 的前 X 次請求不受速度限制，超過後會增加延遲

